<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="406.0" prefWidth="707.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.anasayfa.baran.IstatistiklerController">
   <children>
      <PieChart fx:id="pie" onMouseClicked="#pieAction" prefHeight="306.0" prefWidth="297.0" />
      <BarChart fx:id="barchart" layoutX="305.0" layoutY="14.0" prefHeight="293.0" prefWidth="392.0">
        <xAxis>
          <CategoryAxis label="Şehirler" side="BOTTOM" fx:id="xchart" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="ychart" label="Hasta Sayıları" side="LEFT" upperBound="20.0" />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
